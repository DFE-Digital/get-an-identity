{
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "EnableAdaptiveSampling": false,
    "EnablePerformanceCounterCollectionModule": false
  },
  "DfeAnalytics": {
    "Namespace": "TeacherIdentity.AuthServer"
  },
  "UserVerification": {
    "PinLifetimeSeconds": 900
  },
  "UserVerificationRateLimit": {
    "PinVerificationMaxFailures": 10,
    "PinVerificationFailureTimeoutSeconds": 30,
    "PinGenerationTimeoutSeconds":60,
    "PinGenerationMaxFailures" : 5
  },
  "Serilog": {
    "Enrich": [ "FromLogContext" ]
  },
  "WebHooks": {
    "WebHooksCacheDurationSeconds": 30
  },
  "Zendesk": {
    "UseFakeClient": true
  },
  "Gias": {
    "BaseDownloadAddress": "https://ea-edubase-api-prod.azurewebsites.net/edubase/downloads/public/"
  }
}
